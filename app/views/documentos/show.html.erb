      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="">Documento</a></li>
        <li class="breadcrumb-item active">Caracter√≠sticas</li>
      </ol>

      <h5 class="text">Documento <%= @documento.id%> </h5>
    </div>
    <div class="probar">
      <%= render @documento %>

      <div>
        <div>
        <% if current_user.role =='administrador'%>
